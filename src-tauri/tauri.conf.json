{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm build",
		"beforeDevCommand": "pnpm dev",
		"frontendDist": "../build",
		"devUrl": "http://localhost:5173"
	},
	"bundle": {
		"active": true,
		"category": "Music",
		"copyright": "n3rd",
		"targets": [
			"dmg",
			"app",
			"deb",
			"rpm",
			"msi",
			"nsis"
		],
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"linux": {
			"deb": {
				"depends": []
			},
			"appimage": {
				"bundleMediaFramework": true
			}
		},
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": "",
			"webviewInstallMode": {
				"type": "downloadBootstrapper"
			},
			"wix": {
				"language": "en-US"
			}
		},
		"longDescription": "Get synchronized song lyrics and sing along with your favorite songs.",
		"macOS": {
			"entitlements": null,
			"exceptionDomain": "",
			"frameworks": [],
			"providerShortName": null,
			"signingIdentity": "null"
		},
		"resources": [],
		"shortDescription": "Get synchronized song lyrics.",
		"externalBin": [
			"binaries/playerctl"
		]
	},
	"productName": "ohun",
	"version": "3.0.0",
	"identifier": "com.n3rd.ohun",
	"app": {
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "ohun",
				"width": 800,
				"transparent": true,
				"decorations": true,
				"minWidth": 800,
				"minHeight": 600,
				"titleBarStyle": "Overlay",
				"hiddenTitle": true
			}
		],
		"security": {
			"csp": null,
			"capabilities": [
				"migrated"
			]
		}
	}
}